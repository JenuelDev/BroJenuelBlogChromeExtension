{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build --emptyOutDir",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "zip": "powershell -ExecutionPolicy Bypass -File zip-extension.ps1",
    "pub": "yarn build && yarn zip"
  },
  "dependencies": {
    "@unocss/reset": "^0.61.9",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1",
    "typescript": "~5.0.4",
    "unocss": "^0.52.4",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  }
}